{
  "name": "rnboilerplate",
  "version": "0.0.1",
  "private": true,
  "scripts": {
    "android": "react-native run-android",
    "ios": "react-native run-ios",
    "start": "react-native start",
    "test": "jest",
    "lint": "eslint . --ext .js,.jsx,.ts,.tsx",
    "manifest": "fbt-manifest --src src/ --enum-manifest i18n/fbt/.enum_manifest.json --src-manifest i18n/fbt/.src_manifest.json",
    "collect-fbts": "fbt-collect --hash-module fb-tiger-hash --react-native-mode --manifest < i18n/fbt/.src_manifest.json > i18n/fbt/.source_strings.json",
    "translate-fbts": "fbt-translate --jenkins --source-strings i18n/fbt/.source_strings.json --translations i18n/fbt/translationScriptInput/*.json > i18n/fbt/translatedFbts.json",
    "generate-android-fbt": "babel-node i18n/scripts/generate-android-localizables-executor.js",
    "postinstall": "node scripts/path.js"
  },
  "dependencies": {
    "@babel/register": "^7.17.0",
    "@fbtjs/default-collection-transform": "^0.0.3",
    "@react-navigation/native": "^6.0.6",
    "@react-navigation/stack": "^6.0.11",
    "@reduxjs/toolkit": "^1.7.1",
    "@types/fbt": "^0.16.3",
    "babel-plugin-fbt": "^0.20.3",
    "babel-plugin-fbt-runtime": "^0.9.18",
    "fbt": "^0.17.0",
    "react": "17.0.2",
    "react-native": "0.67.1",
    "react-native-fbt": "^0.0.7",
    "react-native-gesture-handler": "^2.2.0",
    "react-native-safe-area-context": "^3.2.0",
    "react-native-safe-area-view": "^1.1.1",
    "react-native-screens": "^3.10.2",
    "react-native-splash-screen": "^3.3.0"
  },
  "devDependencies": {
    "@babel/core": "^7.16.12",
    "@babel/runtime": "^7.16.7",
    "@react-native-community/eslint-config": "^3.0.1",
    "@types/jest": "^27.4.0",
    "@types/react-native": "^0.66.15",
    "@types/react-test-renderer": "^17.0.1",
    "babel-jest": "^27.4.6",
    "babel-preset-react-native": "^4.0.1",
    "eslint": "^8.7.0",
    "fb-tiger-hash": "^0.1.6",
    "jest": "^27.4.7",
    "metro-react-native-babel-preset": "^0.67.0",
    "react-redux": "^7.2.6",
    "react-test-renderer": "17.0.2",
    "shelljs": "^0.8.5",
    "typescript": "^4.5.5"
  },
  "resolutions": {
    "@types/react": "^17"
  },
  "jest": {
    "preset": "react-native",
    "moduleFileExtensions": [
      "ts",
      "tsx",
      "js",
      "jsx",
      "json",
      "node"
    ]
  }
}
